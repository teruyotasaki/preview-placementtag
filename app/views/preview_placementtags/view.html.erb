<% provide(:title, "View") %>
    <h1>Help</h1>
    <p>
      ここで結果を表示する。
    </p>

<% headers,lines = csv_split(params[:file]) %>
<!-- %= headers % -->
<!-- %= lines % -->
<!-- %= csv_split(params[:file]) % -->

<table class="table table-bordered table-striped" id="table_id">
  <thead>
    <tr>
      <% headers.each do |header| %>
      <th><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% lines.each do |line| %>
    <tr>
      <%
        count = 0
        placementid = ""
      %>
      <% line.each do |sel| %>
        <% if count == 0 %>
          <% placementid = sel %>
        <% end %>
        <% if count == 28 %>
          <td><p><%= sel %></p>
              <p><%= placementid %></p>
              <p id="<%= placementid %>">
                <!-- div class="EBG">hogehoge</div -->
                <%= sel.html_safe %></p>
              <!-- $("#id名 要素名") -->
          </td>
        <% else %>
          <td><%= sel %></td>
        <% end %>
        <% count = count + 1 %>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>

